<%= render '/shared/content_title', title: 'Log In' %>

<%= form_tag '/login' do %>
<div class="well">
	<div class="control-group">
		<%= label_tag :username %>
		<%= text_field_tag :username %>
	</div>

	<div class="control-group">
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</div>

	<br>

	<%= submit_tag 'Login' %>
</div>
<% end %>